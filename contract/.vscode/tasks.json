{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo",
			"args": [
				"+nightly",
				"test"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "cargo +nightly test"
		},
		{
			"type": "shell",
			"command": ". build.sh",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "build.sh"
		}
	]
}